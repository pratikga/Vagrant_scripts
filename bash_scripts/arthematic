#/bin/bash

a=10
b=20

#Addition

echo "the value of a is $a and value of b is $b"
echo "the sum a and b is `expr $a + $b`"
echo "the difference between b and a is `expr $b - $a`"
echo "the value of a time b is `expr $a \* $b`"
echo "the modulos operator `expr $a % $b`"
echo "the value after exchangin is a=$b"
echo "euality operator [ $a == $b ]"
echo "not equality operator [ $a != $b ]"
